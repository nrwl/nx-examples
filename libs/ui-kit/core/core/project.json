{
  "name": "ui-kit-core-core",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui-kit/core/core/src",
  "prefix": "ui-kit",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/ui-kit/core/core/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui-kit/core/core/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ui-kit/core/core/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ui-kit/core/core/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/ui-kit/core/core/**/*.ts",
          "libs/ui-kit/core/core/**/*.html"
        ]
      }
    }
  }
}
